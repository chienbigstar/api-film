<div id="page-content-wrapper">
  <div class="container-fluid">
    <%= form_for @page, class: 'form-group' do |f| %>
        <label>name</label>
        <%= f.text_field :name %><br>
        <label>Type</label>
        <%= f.select :coin, Page.coins.collect {|k, v| [k, k]} %><br>
        <label>description</label>
        <%= f.text_field :description %><br>
        <label>content</label>
        <%= f.text_area :content, rows: 15, cols: 100 %><br>
        <%= f.submit :OK %>
    <% end %>
  </div>
</div>
